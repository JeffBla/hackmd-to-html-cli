{
  "name": "hackmd-to-html-cli",
  "version": "1.0.0",
  "description": "A node.js CLI tool for converting HackMD markdown to HTML.",
  "keywords": [
    "hackmd",
    "markdown",
    "html",
    "cli",
    "converter"
  ],
  "homepage": "https://github.com/ksw2000/hackmd-to-html-cli.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/ksw2000/hackmd-to-html-cli.git"
  },
  "bin": {
    "hmd2html": "dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "example": "npx ts-node lib/cli.ts -i ./example/*",
    "example_dark": "npx ts-node lib/cli.ts -i ./example/index.md -o output/index.dark.html --dark",
    "test": "jest",
    "lint": "npx eslint lib/**"
  },
  "author": "Kashiwa",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^16.11.11",
    "commander": "^11.1.0",
    "deflate-js": "^0.2.3",
    "glob": "^10.3.10",
    "htmlencode": "^0.0.4",
    "markdown-it": "^12.2.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-mathjax3": "^4.3.2",
    "markdown-it-ruby": "^0.1.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "papaparse": "^5.4.1",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  }
}
